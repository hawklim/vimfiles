<?php
class SellerAction extends Action {

    function __construct() {
        parent::__construct();
    }
	
	public function index() {
		$page = array(
					'pageNum' => '2',
					'numPerPage' => '20',
					'orderField' => 'id',
				);
		
		$this->assign('page', $page);
		$this->display();
	}
	
	public function matchSearch() {
		$page = array(
					'pageNum' => '2',
					'numPerPage' => '20',
					'orderField' => 'id',
				);
		
		$this->assign('page', $page);
		$this->display();			
	}
	
	public function detail() {
		$this->display();
	}
}